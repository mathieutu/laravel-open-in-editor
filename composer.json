{
    "name": "mathieutu/laravel-open-in-editor",
    "description": "Let the \"Open in Editor\" feature from browser devtools works with Laravel.",
    "keywords": [
        "laravel-open-in-editor",
        "vuejs",
        "devtools"
    ],
    "homepage": "https://github.com/mathieutu/laravel-open-in-editor",
    "license": "MIT",
    "authors": [
        {
            "name": "Mathieu TUDISCO",
            "email": "dev@mathieutu.ovh"
        }
    ],
    "require": {
        "php": "^7.1",
        "laravel/laravel": ">= 5.5",
        "filp/whoops": "~2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "squizlabs/php_codesniffer": "^3.2",
        "orchestra/testbench": "3.5.*"
    },
    "autoload": {
        "psr-4": {
            "MathieuTu\\LaravelOpenInEditor\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MathieuTu\\LaravelOpenInEditor\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "MathieuTu\\LaravelOpenInEditor\\ServiceProvider"
            ]
        }
    }
}
